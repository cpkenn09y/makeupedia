<%= javascript_include_tag 'back-home' %>
<div class="div-transparent">
  <%= form_tag("/wiki", :method => "get", :class => "main-form") do %>
    <%= text_field_tag "search_page", nil, :type => "search", :id => "search", :name => "ending", placeholder: "Enter search term" %><br>
    <%= submit_tag "Submit", 'id' =>'main-form-submit', 'data-role' => "button", 'data-inline'=> "true", 'data-theme' => "b" %>
  <% end %>
</div>
<% if current_user %>
  <div class="panel panel-default">
    <% current_user.pages.each do |page| %>
      <% @page = page %>
      <%= render 'pages/show' %>
    <% end %>
  </div>
<% end %>

<div class="wikipage"></div>
