<% page_user = @page.page_users.where('user_id=?',current_user.id).first %>
<div class="panel-heading">
  <h4 class="panel-title full-width">
    <a class="accordion-toggle" data-toggle="collapse" data-parent="#accordion" href="#<%= @page.id %>">
      <%= @page.ending %>
    </a><%= link_to "[permalink]", page_user.permalink, :class => "permalink" %>
    <%= link_to 'X', page_user_path(page_user), :confirm => 'Are you sure you want to delete this page?', :class => 'delete', :method => :delete %>  </h4>
</div>
<div id="<%= @page.id %>" class="panel-collapse collapse">
  <div class="panel-changes">
      <% page_user.changes.each do |change| %>
        <% @change = change %>
        <%= render 'changes/show' %>
      <% end %>
      <p class="acc-changes"><%= link_to "Apply all changes", wiki_path(@page.id), class: "all-changes", remote: true %></p>
  </div>
</div>